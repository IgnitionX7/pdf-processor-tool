{
  "version": 2,
  "buildCommand": "npm run build",
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "functions": {
    "main.py": {
      "includeFiles": "{frontend/dist/**,routes/**,*.py}"
    }
  }
}
