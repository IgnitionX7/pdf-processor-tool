{
  "buildCommand": "cd frontend && npm install && npm run build && cd ..",
  "installCommand": "pip install -r requirements.txt",
  "functions": {
    "api/index.py": {
      "includeFiles": "**"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ]
}
