{
  "buildCommand": "cd frontend && npm install && npm run build && cd ..",
  "installCommand": "pip install -r requirements.txt",
  "functions": {
    "api/index.py": {
      "includeFiles": "**"
    }
  },
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/api/index"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index"
    },
    {
      "src": "/docs",
      "dest": "/api/index"
    },
    {
      "src": "/redoc",
      "dest": "/api/index"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index"
    }
  ]
}
